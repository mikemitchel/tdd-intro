<can-import from="~/note-board/"></can-import>
<can-import from="~/note-panel/"></can-import>

<h1>{{ this.message }}</h1>

<div class="app-container">
  <div class="note-list">
    <note-panel notesList:to="notesList"></note-panel>
  </div>
  <div class="sticky-notes">
    <note-board notesList:from="notesList"></note-board>
  </div>
</div>
