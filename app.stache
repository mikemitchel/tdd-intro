<can-import from="~/note-board/"></can-import>
<can-import from="~/note-panel/"></can-import>

<h1>{{ this.message }}</h1>

<div class="app-container">
  <div class="note-panel">
    <note-panel
      notesList:to="notesList"
      setActive:to="setActive">
    </note-panel>
  </div>
  <div class="note-board">
    <note-board
      notesList:from="notesList"
      setActive:from="setActive">
    </note-board>
  </div>
</div>
