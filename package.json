{
  "name": "tdd-intro",
  "version": "1.0.0",
  "description": "Test Driven Dev/Design",
  "main": "app.js",
  "dependencies": {
    "can": "^6.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "http-server": "^0.12.1",
    "standard": "^14.3.1",
    "steal": "^2.2.4",
    "steal-css": "^1.3.2",
    "steal-mocha": "^2.0.1",
    "steal-stache": "^5.0.0",
    "steal-tools": "^2.2.6",
    "testee": "^0.10.2"
  },
  "scripts": {
    "start": "http-server -c-1",
    "test": "npm run lint && testee --reporter Spec --browsers firefox test/index.html",
    "lint": "standard '*.js' '**/*.js' --fix --env mocha"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikemitchel/tdd-intro.git"
  },
  "steal": {
    "babelOptions": {
      "plugins": [
        "transform-class-properties"
      ]
    },
    "paths": {
      "can-route*src/routedata": "node_modules/can-route/src/routedata-definemap.js"
    },
    "plugins": [
      "can",
      "steal-css",
      "steal-stache"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikemitchel/tdd-intro/issues"
  },
  "homepage": "https://github.com/mikemitchel/tdd-intro#readme"
}
